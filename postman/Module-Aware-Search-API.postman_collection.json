        },
        {
          "name": "Search with Filters (Veg + Price)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/search?q=pizza&module_id=4&veg=true&price_min=100&price_max=500&rating_min=4",
              "host": ["{{baseUrl}}"],
              "path": ["search"],
              "query": [
                { "key": "q", "value": "pizza" },
                { "key": "module_id", "value": "4" },
                { "key": "veg", "value": "true", "description": "Vegetarian only" },
                { "key": "price_min", "value": "100" },
                { "key": "price_max", "value": "500" },
                { "key": "rating_min", "value": "4", "description": "Min 4-star rating" }
              ]
            },
            "description": "Search vegetarian pizza between â‚¹100-500 with min 4-star rating."
          }
        },
        {
          "name": "Search with Pagination",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/search?q=biryani&module_id=4&page=2&size=10",
              "host": ["{{baseUrl}}"],
              "path": ["search"],
              "query": [
                { "key": "q", "value": "biryani" },
                { "key": "module_id", "value": "4" },
                { "key": "page", "value": "2", "description": "Page 2" },
                { "key": "size", "value": "10", "description": "10 results per page" }
              ]
